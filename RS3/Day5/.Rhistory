BrainpH <- read_csv("BrainpH.csv")
head(BrainpH)
plot(BrainpH$pH~BrainpH$Age,
main= "scatterplot of relationship between pH and age",
xlab= "Age",
ylab= "pH")
reg = lm(BrainpH$pH~BrainpH$Age)
abline(reg, col = "blue")
summary(reg)
# Because the p-value is big, we fail to reject null that there is no linear relationship between age and pH
#15
Cereal <- read_csv("Cereal.csv")
plot(Cereal$Calories~Cereal$Sugar)
reg = lm(Cereal$Calories~Cereal$Sugar)
abline(reg, col = "purple4")
summary(reg)
#p-value is small --> reject null and say that positive relationship exists between calories and sugar
confint(reg, level = 0.95)
#95% confident that the true average increase in calories per increase in sugar falls between 1.04 and 3.92
