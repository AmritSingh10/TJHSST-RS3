Dementia <- read_csv("LewyDLBad.csv")
plot(Dementia$MMSE~Dementia$APC)
reg = lm(MMSE~APC, data = Dementia)
abline(reg, col = "green3")
summary(reg)
#r-squared is like 0.36 which is not good --> so not strong correlation
resid <- resid(reg)
plot(fitted(reg), resid)
abline(0,0)
#estimated slope = 1.3444, standard error = 0.4225
confint(reg, level = 0.9)
#Interval does not contain zero, p-value less than 0.1
