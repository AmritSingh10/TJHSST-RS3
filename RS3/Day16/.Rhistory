library(readr)
library(ggplot2)
library(dplyr)
library(car)
library(mosaic)
Meniscus <- read_csv("Meniscus.csv")
oil <- read_csv("OilDeapsorbtion.csv")
#53:
# a.)
oil$Oil <- factor(oil$Oil)
stripchart(diff~ Oil, data = oil,
method = "stack",
pch = 19,
xlab = "Difference (Experimental - Control)",
main = "Dotplot of Difference by Oil Level")
favstats(diff~ Oil, data = oil)
# From the dotplot, there is a difference in the amount of oil de-adsorbed between the experimental and control.
# b.)
model_oil <- aov(diff~ factor(Oil), data = oil)
t.test(diff~ Oil, data = oil, var.equal = TRUE)
# P-value is 0.006 which is less than 0.05. Therefore, we reject H0 and evidence shows that there is a difference.
# c.)
anova_oil <- aov(diff ~ factor(Oil), data = oil)
par(mfrow = c(2, 2))
plot(anova_oil)
par(mfrow = c(1, 1))
summary(anova_oil)
# Residuals are normally and equal, QQ-Plot is linear so conditions are met. P is 0.006 so we reject H0.
# d.)
# The conclusions are the same
